@model List<hlcWeb.Models.Doctor>

<div>
 <h3>Search results</h3>
 <table class="table table-bordered table-striped">
     <thead>
         <tr>
             <th>Last Name</th>
             <th>First Name</th>
             <th>Attitude</th>
         </tr>
     </thead>
     <tbody>
         @foreach (var d in @Model)
         {
            <tr>
                <td>@d.LastName</td>
                <td>@d.FirstName</td>
                @*<td><i class="@d.AttitudeText fa @d.AttitudeIcon" title="@d.AttitudeText"></i> <span class="hidden-xs">@d.AttitudeText</span></td>*@
                <td>
                    <span class="@d.AttitudeText fa-stack">
                        <i class="fa fa-circle fa-stack-2x"></i>
                        <i class="fa @d.AttitudeIcon fa-stack-1x fa-inverse" title="@d.AttitudeText"></i>
                    </span>
                    <span class="hidden-xs">@d.AttitudeText</span>
                </td>
            </tr>
         }
     </tbody>
 </table>
</div>
